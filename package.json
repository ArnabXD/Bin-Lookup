{
  "name": "@arnabxd/bin-lookup",
  "version": "1.0.3",
  "description": "Search bin details from various bin database",
  "main": "lib/index.js",
  "scripts": {
    "test": "rimraf lib && tsc --project tsconfig.json && ava",
    "prepublishOnly": "rimraf lib && tsc --project tsconfig.build.json",
    "build": "rimraf lib && tsc --project tsconfig.build.json",
    "lint": "eslint ./src --max-warnings=0",
    "format": "prettier --write src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XDBots/bin-lookup.git"
  },
  "keywords": [
    "bin"
  ],
  "author": "@ArnabXD",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/XDBots/bin-lookup/issues"
  },
  "homepage": "https://github.com/XDBots/bin-lookup#readme",
  "typings": "src/types.d.ts",
  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.10",
    "emoji-flags": "^1.3.0",
    "iso-3166-1": "^2.1.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/emoji-flags": "^1.3.0",
    "@types/node": "^16.11.33",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "ava": "^4.2.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "esm": "^3.2.25",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      },
      "extensions": [
        "ts"
      ],
      "compile": false
    }
  }
}
